public class Student{
    String major;
    int score;
    int studentNumber;
    public String getMajor() {
        return major;
    }
    public void setMajor(String major) {
        this.major = major;
    }
    public int getScore() {
        return score;
    }
    public void setScore(int score) {
        this.score = score;
    }
    public int getstudentNumber;() {
        return studentNumber;
    }
    public void setStudentNumber(int studentNumber) {
        this.studentNumber = studentNumber;
    }

    public Student(){
        super();
        major = "";
        score = 0;
        studentNumber = 0;
    }

    public Student(String name, int age, String majot, int score, int studentNumber){
        super(name,age);
        this.major = major;
        this.score = score;
        this.studentNumber = studentNumber;
    }

}